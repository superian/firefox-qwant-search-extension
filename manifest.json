{
  "manifest_version": 2,
  "name": "Qwant search",
  "short_name": "qw_search",
  "description": "Adds Qwant search as a search engine.",
  "version": "1.0.0",
  "icons": {
    "64": "icons/qwant-icon.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Qwant search",
      "keyword": "@qwant",
      "favicon_url": "https://qwant.com/favicon.ico",
      "search_url": "https://qwant.com/?q={searchTerms}"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "StartpageSearchExtension@superian",
      "strict_min_version": "55.0"
    }
  },
  "author": "superian",
  "homepage_url": "https://github.com/superian/firefox-qwant-search-extension"
}
